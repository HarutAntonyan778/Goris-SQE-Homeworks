class Reverse 
{ 
    static int reversDigits(int tiv) 
    { 
          int hak_num = 0; 
        while(tiv > 0) 
        { 
            hak_num = hak_num * 10 + tiv % 10; 
            tiv = tiv / 10; 
        } 
        return hak_num; 
    } 
    public static void main (String[] args)  
    { 
        int tiv = 56789; 
        System.out.println(reversDigits(tiv)); 
    } 
} 
